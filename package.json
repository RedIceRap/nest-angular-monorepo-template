{
  "name": "nest-angular-monorepo-template",
  "version": "1.0.0",
  "description": "A starting template for nest-angular mono repos.",
  "main": "index.js",
  "scripts": {
    "nest:prebuild": "cd apps/api && rimraf dist",
    "nest:build": "cd apps/api && nest build",
    "nest:format": "cd apps/api && prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "nest:start": "cd apps/api && nest start",
    "nest:start:dev": "cd apps/api && nest start --watch",
    "nest:start:debug": "cd apps/api && nest start --debug --watch",
    "nest:start:prod": "cd apps/api && node dist/main",
    "nest:lint": "cd apps/api && eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "nest:test": "cd apps/api && jest",
    "nest:test:watch": "cd apps/api && jest --watch",
    "nest:test:cov": "cd apps/api && jest --coverage",
    "nest:test:debug": "cd apps/api && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "nest:test:e2e": "cd apps/api && jest --config ./test/jest-e2e.json",
    "angular:ng": "cd apps/fe && ng",
    "angular:start": "cd apps/fe && ng serve",
    "angular:build": "cd apps/fe && ng build",
    "angular:watch": "cd apps/fe && ng build --watch --configuration development",
    "angular:test": "cd apps/fe && ng test"
  },
  "keywords": [
    "nestjs",
    "angular"
  ],
  "author": "Alexander Gerlach",
  "license": "ISC"
}
